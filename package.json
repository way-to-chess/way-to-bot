{
  "name": "way-to-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm --recursive build",
    "dev": "pnpm --recursive dev",
    "lint": "pnpm --recursive lint",
    "server:build": "pnpm --filter @way-to-bot/server build",
    "server:dev": "pnpm --filter @way-to-bot/server dev",
    "adminui:build": "pnpm --filter @way-to-bot/adminui build",
    "adminui:dev": "pnpm --filter @way-to-bot/adminui dev",
    "webapp:build": "pnpm --filter @way-to-bot/webapp build",
    "webapp:dev": "pnpm --filter @way-to-bot/webapp dev",
    "webapp:dev:local": "pnpm --filter @way-to-bot/webapp dev:local",
    "dev:web:all": "pnpm --parallel --filter @way-to-bot/adminui dev --filter @way-to-bot/webapp dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.16.5",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "9.12.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "prettier-eslint-cli": "^8.0.1",
    "typescript": "5.6.3"
  }
}
